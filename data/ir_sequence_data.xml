<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <data noupdate="1">

        <record id="sequence_peserta" model="ir.sequence">
            <field name="name">Urutan Peserta</field>
            <field name="code">seq_peserta</field>
            <field name="prefix">A/%(y)s/%(month)s/</field>
            <field name="padding">4</field>
            <field name="number_increment">1</field>
            <field name="implementation">standard</field>
        </record>


        <record id="sequence_pendaftaran" model="ir.sequence">
            <field name="name">Urutan Pendaftaran</field>
            <field name="code">seq_pendaftaran</field>
            <field name="prefix">REG/%(y)s/%(month)s/</field>
            <field name="padding">4</field>
            <field name="number_increment">1</field>
            <field name="implementation">standard</field>
        </record>
    </data>

</odoo>
